node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*
.npm

# Build output
dist/
build/
electron-cache/
out/

# Logs
*.log
logs/

# IDE
.vscode/
.idea/
*.swp
*.swo
*~

# OS
.DS_Store
Thumbs.db

# Git
.git/
.gitignore

# Temp
*.tmp
.cache/
.temp/

